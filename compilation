gcc src/program.c -o obj/program -lgsl -lgslcblas -lm

// GNU Scientific Library
#include <gsl/gsl_vector.h>
#include <gsl/gsl_matrix.h>
#include <gsl/gsl_blas.h>

gcc program.c -o program -lopenblas -lm

// C interface for Basic Linear Algebra Subprograms
#include <cblas.h>

// compilation
gcc src/mainOptimization.c include/LayerDense.h -o obj/mainOptimization -lopenblas -lm
